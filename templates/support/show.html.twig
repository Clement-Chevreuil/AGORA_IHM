{% extends 'base.html.twig' %}

{% block title %}Support{% endblock %}

{% block body %}
    <h1>Support</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ support.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ support.title }}</td>
            </tr>
            <tr>
                <th>Information</th>
                <td>{{ support.information }}</td>
            </tr>
            <tr>
                <th>Vu</th>
                <td>{{ support.vu ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ support.status }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('support_index') }}">back to list</a>

    <a href="{{ path('support_edit', {'id': support.id}) }}">edit</a>

    {{ include('support/_delete_form.html.twig') }}
{% endblock %}
